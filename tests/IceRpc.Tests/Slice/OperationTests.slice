module IceRpc::Slice::Tests;

trait MyTrait;

struct MyStruct
{
    x:int,
    y:int,
}

interface MyOperations
{
    // Single input parameter and return value
    opInt(p1: int) -> int;
    opString(p1: string) -> string;
    opMyStruct(p1: MyStruct) -> MyStruct;
    opMyTrait(p1: MyTrait) -> MyTrait;

    // Multiple input parameters and return values
    opIntAndString(p1: int, p2: string) -> (r1: int, r2: string);

    // Optional input parameters and return values
    opOptional(p1: int, p2: string, p3: int?, p4: string?) ->
        (r1: int, r2: string, r3: int?, r4: string?);

    // Tagged input parameters and return values
    opTagged(p1: int, p2: string, p3: tag(1) int?, p4: tag(2) string?) ->
        (r1: int, r2: string, r3: tag(1) int?, r4: tag(2) string?);
}
