module IceRpc::IntegrationTests;

struct MyStructA
{
    a: bool,
    b: int8,
    c: uint8,
    d: int16,
    e: uint16,
    f: int32,
    g: uint32,
    h: varint32,
    i: varuint32,
    j: int64,
    k: uint64,
    l: varint62,
    m: varuint62,
    n: float32,
    o: float64,
    p: string
}

struct MyStructWithOptionalMembersA
{
     a: bool?,
     b: int8?,
     c: uint8?,
     d: int16?,
     e: uint16?,
     f: int32?,
     g: uint32?,
     h: varint32?,
     i: varuint32?,
     j: int64?,
     k: uint64?,
     l: varint62?,
     m: varuint62?,
     n: float32?,
     o: float64?,
     p: string?
}

struct MyStructWithTaggedMembersA
{
    a: tag (1) bool?,
    b: tag (2) int8?,
    c: tag (10) uint8?,
    d: tag (20) int16?,
    e: tag (11) uint16?,
    f: tag (21) int32?,
    g: tag (31) uint32?,
    h: tag (41) varint32?,
    i: tag (32) varuint32?,
    j: tag (42) int64?,
    k: tag (33) uint64?,
    l: tag (43) varint62?,
    m: tag (1024) varuint62?,
    n: tag (2048) float32?,
    o: tag (4096) float64?,
    p: tag (8192) string?
}

interface StructOperations
{
    op1(p1: MyStructA) -> MyStructA;
    op2(p1: MyStructWithOptionalMembersA) -> MyStructWithOptionalMembersA;
    op3(p1: MyStructWithTaggedMembersA) -> MyStructWithTaggedMembersA;
}
