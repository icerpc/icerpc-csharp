module IceRpc::Tests::Slice;

enum MyEnum
{
    enum1,
    enum2,
    enum3,
}

enum MyFixedLengthEnum : int16
{
    senum1,
    senum2,
    senum3
}

enum MyEnumWithCustomEnumerators
{
    enum1 = -10,
    enum2 = 20,
    enum3 = 30,
}

[cs::attribute("System.Flags")] unchecked enum MyUncheckedEnum : uint32
{
    E0 = 1,
    E1 = 2,
    E2 = 4,
    E3 = 8,
    E4 = 16,
}

interface EnumOperations
{
    opCheckedEnumSeq(p1: sequence<MyEnum>);
    opCheckedEnumWithFixedLengthSeq(p1: sequence<MyFixedLengthEnum>);
    opUncheckedEnumSeq(p1: sequence<MyUncheckedEnum>);
}
