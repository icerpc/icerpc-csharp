<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AssemblyName>Ice</AssemblyName>
        <Title>Ice .NET library</Title>
        <OutputPath>../../lib</OutputPath>
        <TargetFramework>net5.0</TargetFramework>
    </PropertyGroup>
    <ItemGroup>
        <SliceCompile Include="../../../slice/$(AssemblyName)/*.ice" Exclude="../../../slice/$(AssemblyName)/*F.ice;../../../slice/$(AssemblyName)/BuiltinSequences.ice;../../../slice/$(AssemblyName)/Context.ice" />
        <SliceCompile Remove="..\..\..\slice\Ice\LocatorDiscovery.ice" />
        <Compile Update="generated\Discovery.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Discovery.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Identity.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Identity.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Locator.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Locator.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Metrics.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Metrics.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Process.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Process.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Encoding.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Encoding.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Endpoint.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Endpoint.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Exceptions.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Exceptions.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Ice1Definitions.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Ice1Definitions.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Ice2Definitions.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Ice2Definitions.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Protocol.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Protocol.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\ProxyEncoding.cs">
          <SliceCompileSource>..\..\..\slice\Ice\ProxyEncoding.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\Retryable.cs">
          <SliceCompileSource>..\..\..\slice\Ice\Retryable.ice</SliceCompileSource>
        </Compile>
        <Compile Update="generated\SlicDefinitions.cs">
          <SliceCompileSource>..\..\..\slice\Ice\SlicDefinitions.ice</SliceCompileSource>
        </Compile>
    </ItemGroup>
    <ItemGroup>
      <PackageReference Include="StyleCop.Analyzers" Version="1.2.0-beta.261" PrivateAssets="All"/>
      <PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="5.0.3" />
      <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.0-beta-20204-02" PrivateAssets="All"/>
    </ItemGroup>
</Project>
