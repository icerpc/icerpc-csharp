module IceRpc::Tests::Slice;

exception CompressMyException
{
    bytes: sequence<uint8>,
}

interface CompressTest
{
    [compress(args)] opCompressArgs(size: int, p1: sequence<uint8>);
    [compress(return)] opCompressReturn(size: int) -> sequence<uint8>;
    [compress(args, return)] opCompressArgsAndReturn(p1: sequence<uint8>) -> sequence<uint8>;
    [compress(args, return)] opWithUserException(size: int);

    [compress(args)] opCompressStreamArg(p1: stream uint8) -> int;
    [compress(return)] opCompressReturnStream(size: int) -> stream uint8;
    [compress(args, return)] opCompressStreamArgAndReturnStream(p1: stream uint8) -> stream uint8;
}
