{
  "metadata":[
    {
      "src":[
        {
          "src":"../",
          "files":[
            "src/IceRpc/bin/**/IceRpc.dll",
            "src/IceRpc.Coloc/bin/**/IceRpc.Coloc.dll",
            "src/IceRpc.Deadline/bin/**/IceRpc.Deadline.dll",
            "src/IceRpc.Deflate/bin/**/IceRpc.Deflate.dll",
            "src/IceRpc.Extensions.DependencyInjection/bin/**/IceRpc.Extensions.DependencyInjection.dll",
            "src/IceRpc.Interop/bin/**/IceRpc.Interop.dll",
            "src/IceRpc.Locator/bin/**/IceRpc.Locator.dll",
            "src/IceRpc.Logger/bin/**/IceRpc.Logger.dll",
            "src/IceRpc.Metrics/bin/**/IceRpc.Metrics.dll",
            "src/IceRpc.RequestContext/bin/**/IceRpc.RequestContext.dll",
            "src/IceRpc.Retry/bin/**/IceRpc.Retry.dll",
            "src/IceRpc.Telemetry/bin/**/IceRpc.Telemetry.dll"
          ],
          "exclude":[
            "**/obj/**",
            "**/ref/**"
          ]
        }
      ],
      "dest":"./",
      "includePrivateMembers":false,
      "disableGitFeatures":false,
      "disableDefaultFilter":false,
      "noRestore":false,
      "namespaceLayout":"flattened"
    }
  ],
  "build":{
    "content":[
      {
        "files":[
            "**.yml",
            "index.md"
        ]
      },
    ],
    "resource":[
      {
        "files":[
          "images/**"
        ]
      }
    ],
    "overwrite":[
      {
        "files":[
          "api-overwrite/**.md"
        ],
        "exclude":[
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest":"_site",
    "xrefService":[
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "globalMetadataFiles":[

    ],
    "fileMetadataFiles":[

    ],
    "template":[
      "default",
      "templates/singulinkfx"
    ],
    "postProcessors":[

    ],
    "noLangKeyword":false,
    "keepFileLink":false,
    "disableGitFeatures":false,
    "globalMetadata":{
      "_appTitle":"IceRPC",
      "_appName":"IceRPC",
      "_appFaviconPath":"favicon.png",
      "_appLogoPath":"logo.svg",
      "_appFooter":"<strong>IceRPC</strong>",
      "_copyrightFooter":"Copyright Â© ZeroC, Inc.",
      "_enableSearch":true,
      "_disableSideFilter":false,
      "_enableNewTab":true,
      "_disableContribution":false,
      "_disableBreadcrumb":false
    }
  }
}
