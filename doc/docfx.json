{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
              "src/IceRpc/bin/**/IceRpc.dll",
              "src/IceRpc.Coloc/bin/**/IceRpc.Coloc.dll",
              "src/IceRpc.Interop/bin/**/IceRpc.Interop.dll",
          ],
          "exclude": [
              "**/obj/**",
              "**/ref/**",
              "_site/**"
          ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "articles/**/*.md", "*.md", "toc.yml" ]
      }
    ],
    "resource": [
      {
        "files": [ "articles/images/**"]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "docfx seed website",
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "dest": "_site",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
