{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "src/*/*.csproj",
                    ],
                    "exclude": [
                        "src/IceRpc.Templates/**"
                    ],
                    "src": "../"
                }
            ],
            "dest": "api",
            "includePrivateMembers": false,
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "noRestore": false,
            "namespaceLayout": "flattened"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml"
                ]
            },
            {
                "files": [
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "sitemap": {
            "baseUrl": "https://docs.testing.zeroc.com"
        },
        "overwrite": [
            {
                "files": [
                    "overwrite/**.md",
                    "namespaces/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "modern",
            "template"
        ],
        "postProcessors": ["ExtractSearchIndex"],
        "noLangKeyword": false,
        "keepFileLink": false,
        "disableGitFeatures": false,
        "globalMetadata":{
            "_appTitle":"IceRPC",
            "_appName":"IceRPC",
            "_appFaviconPath":"images/favicon.ico",
            "_appLogoPath":"images/logo.svg",
            "_appFooter":"Â© 2023 ZeroC",
            "_enableSearch": true,
            "_gitContribute": {
                "repo": "https://github.com/icerpc/icerpc-csharp",
                "branch": "main",
                "apiSpecFolder": "docs-overwrite"
            }
        }
    }
}
