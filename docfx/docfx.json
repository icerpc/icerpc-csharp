{
  "metadata": [
    {
      "src": [
        {
          "files": ["src/*/*.csproj"],
          "exclude": ["src/IceRpc.Templates/**", "src/IceRpc.Slice.Generators/**", "src/IceRpc.Protobuf.Generators/**"],
          "src": "../"
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "EnumSortOrder": "declaringOrder"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["api/**.yml"]
      },
      {
        "files": ["toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "sitemap": {
      "baseUrl": "https://docs.icerpc.dev"
    },
    "overwrite": [
      {
        "files": ["overwrite/**.md", "namespaces/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern", "template"],
    "postProcessors": ["ExtractSearchIndex"],
    "noLangKeyword": false,
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "IceRPC",
      "_appName": "Docs",
      "_appFaviconPath": "images/favicon.ico",
      "_appLogoPath": "images/logo.svg",
      "_appLogoUrl": "https://docs.icerpc.dev",
      "_appFooter": "Â© 2023 ZeroC",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/icerpc/icerpc-csharp",
        "branch": "main",
        "apiSpecFolder": "docs-overwrite"
      }
    }
  }
}
